if [ $# -ne 1 ]; then
    echo "Usage: pop -option"
    echo "  -m morning"
    echo "  -n night"
    echo "  -o other"
    exit 1
fi

option=$1
path="/home/sean/Coding/euphony/"

case "$option" in
    '-m') popfile "${path}morning";;
    '-n') popfile "${path}night";;
    '-o') popfile "${path}other";;
    *) echo Unknown option
        exit 1;;
esac
